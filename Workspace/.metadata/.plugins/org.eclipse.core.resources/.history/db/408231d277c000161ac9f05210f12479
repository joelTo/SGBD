		System.out.println("");
		System.out.println("Veuillez entrer votre Nom :");
		String nom = ihmUtilCleint.getScanner().next();
		System.out.println("Veuillez entrer votre Nom :");
		String nom = ihmUtilCleint.getScanner().next();

	}

